{
    "name": "topmcfrance/api-client",
    "description": "TopMcFrance Api client for PHP",
    "keywords": ["topmcfrance", "vote", "minecraft"],
    "type": "library",
    "homepage": "https://github.com/TopMcFrance/api",
    "license": "MIT",
    "authors": [
        {
            "name": "Jewome62",
            "email": "jewome62@gmail.com",
            "role": "Contributor"
        }
    ],
    "require": {
        "php": "^5.6|^7.0"
    },
    
    "require-dev": {
        "phpunit/phpunit": "~5.5"
    },
    "autoload": {
        "psr-4": {
            "TopMcFrance\\Api\\": "src/"
        }
    },
    "autoload-dev": {
